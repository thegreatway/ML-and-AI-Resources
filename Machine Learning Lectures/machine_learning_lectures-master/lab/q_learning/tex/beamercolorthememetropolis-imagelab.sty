\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthememetropolis-imagelab}[2017/06/20 Metropolis color theme]
\usecolortheme{metropolis}

\definecolor{mImagelabRed}{HTML}{C0504D}
\definecolor{mImagelabWhite}{HTML}{FFFFFF}

\setbeamercolor{header text}{%
  fg=mImagelabWhite,
  bg=mImagelabRed
}

\setbeamercolor{palette primary}{%
  use=normal text,
  fg=normal text.bg,
  bg=normal text.fg
}
\setbeamercolor{palette frametitle}{%
  use=header text,
  fg=header text.fg,
  bg=header text.bg
}
\setbeamercolor{frametitle}{%
  use=palette frametitle,
  parent=palette frametitle
}

\setbeamercolor{alerted text}{%
  fg=mImagelabRed
}

\mode<all>
\endinput
